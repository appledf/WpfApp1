<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        xmlns:uc ="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Width="1000" Height="800">
    <Window.Background>
        <ImageBrush ImageSource="bg.png"/>
    </Window.Background>
    <Grid>
        <uc:UCCycleProcessBar x:Name="uCCycleProcessBar" Value="10" Step="10" StartAngle="220" Width="500"></uc:UCCycleProcessBar>
    </Grid>
</Window>
